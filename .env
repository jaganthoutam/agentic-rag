# Application Configuration
APP_ENV=development
APP_DEBUG=true

# Logging Configuration
LOG_LEVEL=info  # Options: debug, info, warning, error, critical
LOG_FORMAT=json  # Options: json, text
LOG_OUTPUT=console  # Options: console, file, both

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Memory Configuration
MEMORY_SHORT_TERM_ENABLED=true
MEMORY_SHORT_TERM_CAPACITY=1000
MEMORY_SHORT_TERM_TTL=3600

MEMORY_LONG_TERM_ENABLED=true
MEMORY_LONG_TERM_CONNECTION_STRING=postgresql://user:password@localhost:5432/memory_db

# Planning Configuration
PLANNING_REACT_ENABLED=true
PLANNING_REACT_MAX_STEPS=10
PLANNING_COT_ENABLED=true
PLANNING_COT_MAX_DEPTH=5

# Agent Configuration
AGENT_AGGREGATOR_ENABLED=true
AGENT_AGGREGATOR_TIMEOUT=30

AGENT_SEARCH_ENABLED=true
AGENT_SEARCH_ENGINE=kagi
SEARCH_API_KEY=xxxxxx-c.xxxxxx

AGENT_LOCAL_DATA_ENABLED=true
AGENT_LOCAL_DATA_PATH=./data

AGENT_CLOUD_ENABLED=true
AGENT_CLOUD_PROVIDER=aws
AWS_REGION=us-west-2
AWS_ACCESS_KEY=your_aws_access_key_here
AWS_SECRET_KEY=your_aws_secret_key_here

AGENT_GENERATIVE_ENABLED=true
AGENT_GENERATIVE_MODEL=claude-3-opus-20240229
CLAUDE_API_KEY=xxxxxxxx
AGENT_GENERATIVE_MAX_TOKENS=4000
AGENT_GENERATIVE_TEMPERATURE=0.7

AGENT_MEMORY_ENABLED=true
